{
    "name": "jonnitto/textwithplyr",
    "type": "neos-plugin",
    "license": "MIT",
    "description": "Text with plyr for Neos CMS",
    "keywords": ["flow", "neos", "youtube", "vimeo", "mp3", "video", "text"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "gulp.json", "package.json", "CODE_OF_CONDUCT.md"]
    },
    "require": {
        "neos/neos": "~3.0 || dev-master",
        "jonnitto/plyr": "^1.2",
        "carbon/filecontent": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\TextWithPlyr\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.TextWithPlyr",
        "neos": {
            "package-key": "Jonnitto.TextWithPlyr"
        }
    }
}
